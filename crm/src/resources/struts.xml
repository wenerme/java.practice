<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE struts PUBLIC
        "-//Apache Software Foundation//DTD Struts Configuration 2.3//EN"
        "http://struts.apache.org/dtds/struts-2.3.dtd">
<struts>

    <package name="userPackage" extends="struts-default" namespace="/">

        <!--定义默认方法 -->
        <default-action-ref name="list" />

        <!--显示列表-->
        <action name="list" method="list" class="me.wener.lesson.practice.crm.sys.user.action.UserAction">
            <result type="dispatcher">/index.jsp</result>
        </action>

        <action name="add" method="add" class="me.wener.lesson.practice.crm.sys.user.action.UserAction">
            <result type="dispatcher">/index.jsp</result>
        </action>
    </package>

</struts>
